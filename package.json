    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.1.6",
    "@types/node": "^16.11.12",
    "gts": "^3.1.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.5.2"
