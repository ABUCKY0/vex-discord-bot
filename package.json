    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.3.1",
    "@types/node": "^16.6.2",
    "gts": "^3.1.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.5"
