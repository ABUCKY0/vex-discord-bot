  "keywords": [
    "Discord",
    "bot",
    "VEX",
    "robotics",
    "competition"
  ],
  "homepage": "https://github.com/jtkiesel/vexibot#readme",
  "bugs": "https://github.com/jtkiesel/vexibot/issues",
  "license": "MIT",
  "author": {
    "name": "Jordan Kiesel",
    "url": "https://github.com/jtkiesel"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtkiesel/vexibot.git"
  },
  "scripts": {
